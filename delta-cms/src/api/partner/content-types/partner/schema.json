{
  "kind": "collectionType",
  "collectionName": "partners",
  "info": {
    "singularName": "partner",
    "pluralName": "partners",
    "displayName": "Partenaire",
    "description": "Partenaires et collaborateurs"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": ["images"]
    },
    "website": {
      "type": "string",
      "required": false
    },
    "description": {
      "type": "text",
      "required": false
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "assurance",
        "fournisseur",
        "clinique",
        "laboratoire"
      ],
      "default": "fournisseur"
    },
    "order": {
      "type": "integer",
      "default": 0
    },
    "isActive": {
      "type": "boolean",
      "default": true
    }
  }
} 